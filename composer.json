{
    "name": "melsaka/laravel-firebase-messaging",
    "description": "Laravel package for Firebase Cloud Messaging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed ElSaka",
            "email": "melsakax@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "kreait/laravel-firebase": "^6.1.0"
    },
    "autoload": {
        "psr-4": {
            "Melsaka\\LaravelFirebaseMessaging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Melsaka\\LaravelFirebaseMessaging\\Providers\\FirebaseMessagingServiceProvider"
            ],
            "aliases": {
                "FirebaseMessaging": "Melsaka\\LaravelFirebaseMessaging\\Facades\\FirebaseMessaging"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}